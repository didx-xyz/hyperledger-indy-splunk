          <latest>$time_token.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">all</option>
        <option name="refresh.display">progressbar</option>
        <drilldown>
          <link target="_blank">search?q=sourcetype%3D%22hyper%3Aindy%3Ajson%22%20%22response.result.data.Pool_info.Unreachable_nodes_count%22%3E0%20%20%7C%20table%20%20name%20%22response.result.data.Pool_info.Unreachable_nodes%7B%7D%7B%7D%22&amp;earliest=-60m@m&amp;latest=now</link>
        </drilldown>
      </single>
    </panel>
  </row>
